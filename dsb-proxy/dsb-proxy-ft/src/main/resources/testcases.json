        "clientCommunicationInfo": {
          "ip": "127.0.0.1",
          "port": 5061,
          "transport": "udp"
        }
      },
      "uac": {
        "myUri" : "sip:uac@nist.gov",
        "ip": "127.0.0.1",
        "port": 5075,
        "transport": "udp",
        "messages": [{
          "type": "request",
          "name": "INVITE",
          "direction": "sends",
          "parameters": {
            "requestParameters": {
              "headerAdditions": {
                "requestUri": "sip:uas@127.0.0.1:7000"
              }
            }
          }
        },
          {
            "type": "response",
            "forRequest": "INVITE",
            "direction": "receives",
            "optional" : true,
            "validation": {
              "responseCode": 180,
              "reasonPhrase": "Ringing"
            }
          },
          {
            "type": "response",
            "forRequest": "INVITE",
            "direction": "receives",
            "validation": {
              "responseCode": 200,
              "reasonPhrase": "OK",
              "record-route": "<sip:rr$n=SampleNetwork@127.0.0.1:5061;transport=udp;lr>"
