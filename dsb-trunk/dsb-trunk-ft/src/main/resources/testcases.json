          "port": 5060,
          "transport": "udp"
        }
      },
      "uac": {
        "myUri" : "sip:uac@nist.gov",
        "ip": "127.0.0.1",
        "port": 5075,
        "transport": "udp",
        "messages": [{
          "type": "request",
          "name": "INVITE",
          "direction": "sends",
          "parameters": {
            "requestParameters": {
